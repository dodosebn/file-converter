generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}
model User {
  id Int @id @default(autoincrement())
  email String @unique
  name String
  password String?
 googleId String?
 githubId String?
  FilesToConvert FilesToConvert[]
}
model FilesToConvert{
  id Int @id @default(autoincrement())
  user User @relation(fields: [userId], references: [id])
  userId Int
  originalName String
  storedName String
  fileType String
  targetType String
  createdAt DateTime @default(now())
  status String @default("pending")

}